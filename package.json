    "d3": "^5.4.0",
    "d3-force": "^1.1.0",
    "electron-is-dev": "^0.3.0",
    "jacquard-runtime": "0.3.x",
    "jacquard-yarncompiler": "0.2.x",
    "jacquard-yarnparser": "^0.3.4",
    "prop-types": "^15.6.0",
    "radium": "^0.24.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-file-reader": "^1.1.4",
    "react-icons": "^2.2.7",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-act": "^1.7.4",
